{
  "name": "vite-basic-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint:eslint": "eslint --max-warnings 0 \"src/**/*.{vue,ts,tsx,js}\""
  },
  "dependencies": {
    "@vueuse/core": "^7.3.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.6",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12",
    "windicss": "^3.3.0"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.0.12",
    "@qianphong/eslint-config": "^0.0.4",
    "@types/node": "^16.11.13",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "eslint": "^8.4.1",
    "prettier": "^2.5.1",
    "sass": "^1.45.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.5.3",
    "unplugin-icons": "^0.12.23",
    "unplugin-vue-components": "^0.17.9",
    "vite": "^2.7.2",
    "vite-plugin-pages": "^0.19.3",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vite-plugin-windicss": "^1.5.4",
    "vue-tsc": "^0.29.8"
  },
  "eslintConfig": {
    "extends": "@qianphong"
  },
  "eslintIgnore": [
    ".vscode",
    ".husky",
    "dist",
    "public",
    "node_modules"
  ],
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "arrowParens": "avoid",
    "quoteProps": "consistent",
    "trailingComma": "all"
  }
}
